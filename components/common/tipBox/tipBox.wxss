/* components/common/tipBox/tipBox.wxss */
.error-tip {
    position:absolute;
    left:50%;
    top:35%;
    transform:translate(-50%,-50%);
    opacity: 0.7;
    background: #111111;
    border-radius: 6rpx;
    font-size:26rpx;
    color:#FFFFFF;
    padding:10rpx 15rpx;
    z-index:99;
    text-align: center;
}